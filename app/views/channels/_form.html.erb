
<%= simple_form_for(@channel) do |f| %>
  <%= f.error_notification %>

 <% if has_role?(:admin)%>
 <div class="field">
   <div class="control">
     <%= f.input :channel, input_html: { class: 'input' }, wrapper: false, label_html: { class: 'label' } %>
   </div>
 </div>

 <%= f.button :submit, class: 'button is-primary' %>
 <%end%>
<% end %>